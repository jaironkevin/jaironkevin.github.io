library(ggplot2)
library(dplyr)
set.seed(123)
n <- 300
data <- data.frame(
log_totexp = rnorm(n, mean = 8, sd = 0.5),
log_income = rnorm(n, mean = 9, sd = 0.6),
age        = rnorm(n, mean = 45, sd = 12),
wtrans     = rnorm(n, mean = 0.3, sd = 0.15),
children_fac = factor(sample(c("No Children", "With Children"), n, replace = TRUE))
)
ggpairs(
data,
columns = c("log_totexp", "log_income", "age", "wtrans"),
title = "Bivariate analysis of synthetic household expenditure data",
upper = list(
continuous = wrap("cor", size = 3)
),
lower = list(
continuous = wrap("smooth", alpha = 0.3, size = 0.1)
),
mapping = aes(color = children_fac)
)
