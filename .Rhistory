# Cargar librerías
library(ggplot2)
# Parámetros de la población
set.seed(123)
n_samples <- 1000  # Número de muestras
sample_size <- c(5, 30, 100)  # Tamaños de muestra
# Generar muestras y calcular medias
sample_means <- lapply(sample_size, function(n) {
means <- replicate(n_samples, mean(rexp(n, rate = 1)))
data.frame(Tamaño = paste("n =", n), Medias = means)
})
# Unir los datos
sample_means_df <- do.call(rbind, sample_means)
# Graficar la distribución de medias muestrales
ggplot(sample_means_df, aes(x = Medias, fill = Tamaño)) +
geom_histogram(aes(y = ..density..), bins = 30, alpha = 0.6, position = "identity") +
geom_density(color = "black", size = 1) +
facet_wrap(~Tamaño, scales = "free") +
labs(title = "Distribución de Medias Muestrales y el Teorema Central del Límite",
x = "Media Muestral", y = "Densidad") +
theme_minimal()
